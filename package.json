{
  "name": "yabai-and-skhd-highlighting",
  "displayName": "yabai and skhd Highlighting",
  "description": "Highlighting for .yabairc and .skhdrc configuration files",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "yabai",
        "aliases": ["yabai"],
        "extensions": [".yabairc", "yabairc"],
        "configuration": "./yabai-language-configuration.json"
      },
      {
        "id": "skhd",
        "aliases": ["skhd"],
        "extensions": [".skhdrc", "skhdrc"],
        "configuration": "./skhd-language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "yabai",
        "scopeName": "source.yabai",
        "path": "./syntaxes/yabai.tmLanguage.json"
      },
      {
        "language": "skhd",
        "scopeName": "source.skhd",
        "path": "./syntaxes/skhd.tmLanguage.json"
      }
    ]
  }
}